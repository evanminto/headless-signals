{
  "name": "@evanminto/headless-signals",
  "version": "0.0.1",
  "main": "./dist/headless-signals/index.js",
  "type": "module",
  "repository": "git@github.com:evanminto/headless-signals.git",
  "author": "Evan Minto <evan.minto@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": "./dist/headless-signals/index.js",
    "./components": "./dist/components/components.js",
    "./tools": "./dist/tools/tools.js",
    "./preact": "./dist/preact/preact.js"
  },
  "scripts": {
    "build": "parcel build",
    "demo:preact": "parcel serve ./demo/preact/index.html"
  },
  "targets": {
    "headless-signals": {
      "source": "./src/index.js",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "context": "browser",
      "includeNodeModules": false
    },
    "components": {
      "source": "./src/components.js",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "context": "browser",
      "includeNodeModules": false
    },
    "tools": {
      "source": "./src/tools.js",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "context": "browser",
      "includeNodeModules": false
    },
    "preact": {
      "source": "./src/preact.js",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "context": "browser",
      "includeNodeModules": false
    }
  },
  "dependencies": {
    "@preact/signals": "^1.2.3",
    "@preact/signals-core": "^1.6.0",
    "preact": "^10.21.0"
  },
  "devDependencies": {
    "htm": "^3.1.1",
    "parcel": "^2.12.0"
  }
}
